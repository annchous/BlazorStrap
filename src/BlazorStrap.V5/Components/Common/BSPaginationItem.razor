@inherits BlazorStrap.Shared.Components.Common.BSPaginationItemBase
@namespace BlazorStrap.V5

<li class="@ClassBuilder" @attributes="Attributes">
    @if (string.IsNullOrEmpty(Url))
    {
        <span class="page-link">
            @ChildContent
        </span>
    }
    else
    {
        <a href="@Url" class="page-link">@ChildContent</a>
    }
</li>